    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/csn/homeworks/homework_3/closeness-centrality-v2/bin/closeness",
            "args": ["${workspaceFolder}/csn/homeworks/homework_3/closeness-centrality-v2/data"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/csn/homeworks/homework_3/closeness-centrality-v2",
            "environment": [],
            "externalConsole": true,
            "MIMode": "lldb"
        },
        {
            "name": "(lldb) Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "enter program name, for example ${workspaceFolder}/a.out",
            "processId": "${command:pickProcess}",
            "MIMode": "lldb"
        }
    ]
